JanelaPrincipal:
    ScreenOne:#Login
    ScreenTwo:#Cadastro
    ScreenThree:#Tela Principal
    ScreenFour:#Horários
    ScreenFive:#Serviços
    ScreenSix:#Esqueceu a senha?
    ScreenAdm:#Tela do Admin

#-----Tela 1, Login-----
<ScreenOne>:
    name:"ScreenOne"
    canvas.before:
        Color:
            rgba:0, 0, 0, 1
        Rectangle:
            pos: self.pos
            size: self.size
    MDBoxLayout:
        size_hint_y: None
        height: "290dp"
        orientation: 'vertical'
        pos_hint:{'center_y':.78}

        FitImage:
            size_hint_y: 5
            source: './img/b.jpeg'
    MDTextField:
        hint_text: "Email"
        mode: "fill"
        pos_hint:{'center_x':.5 , 'center_y':.4}
        size_hint_x:.8
        fill_color_normal: [.44, .43, .43, 0.2]
        line_color_focus: .44, .43, .43, 0.2
        line_color_normal: .44, .43, .43, 1
        radius: 0, 0, 0, 0
        id:email
    MDTextField:
        hint_text: "Senha"
        mode: "fill"
        pos_hint:{'center_x':.5 , 'center_y':.3}
        size_hint_x:.8
        fill_color_normal: [.44, .43, .43, 0.2]
        line_color_focus: .44, .43, .43, 0.2
        line_color_normal: .44, .43, .43, 1
        radius: 0, 0, 0, 0
        id:senha 
    MDRectangleFlatButton:
        text: "Login"
        text_color: 0,0,0,1
        line_color: .2, .1, .0, 1
        md_bg_color:.87, .74, .61, 1
        pos_hint:{'center_x':.5 , 'center_y':.2}
        size_hint_x:.8
        size_hint_y:.07
        on_release:
            app.root.validacao()
    MDRoundFlatButton:
        text: "Cadastre-se"
        text_color: 1, 1, 1, 1
        pos_hint:{'center_x':.5 , 'center_y':.05}
        size_hint_x:.3
        md_bg_color: [0, 0, 0, 1]
        line_color: [0, 0, 0, 0.0]
        on_release:
            app.root.current="ScreenTwo"
            root.manager.transition.direction = "up"
    MDRoundFlatButton:
        text: "Esqueceu a senha ?"
        text_color: 1, 1, 1, 1
        pos_hint:{'center_x':.5 , 'center_y':.1}
        size_hint_x:.3
        md_bg_color: [0, 0, 0, 1]
        line_color: [0, 0, 0, 0.0]
        on_press:app.root.current="ScreenSix"
    MDIconButton:
        icon: "home-roof"
        on_press:
            app.root.current="ScreenAdm"
            app.root.admin()




